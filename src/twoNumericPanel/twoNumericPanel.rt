<form>
  <input type="number" name="lat" placeholder="lat" onChange="(e) => this.latChanged(e.target.value)">
  <input type="number" name="long" placeholder="long" onChange="(e) => this.longChanged(e.target.value)">
  <div>ShaiK is over-reacting at ({this.state.location.long},{this.state.location.lat})</div>
</form>